{
  "cargo": {
    "features": "all",
    "runBuildScripts": true,
    "buildScripts": {
      "enable": true
    }
  },
  "typing": {
    "autoClosingAngleBrackets": true
  },
  "numThreads": 6,
  "cachePriming": {
    "enable": true,
    "numThreads": 4
  },
  "diagnostics": {
    "enable": true
  },
  "checkOnSave": true,
  "check": {
    "ignore": ["inactive-code"],
    "allTargets": false,
    "workspace": true,
    "invocationLocation": "once",
    "invocationStrategy": "once",
    "overrideCommand": "cargo check --message-format=json-diagnostic-rendered-ansi"
  },
  "server": {
    "extraEnv": {
      "RA_LOG": null,
      "RUST_BACKTRACE": null
    }
  },
  "procMacro": {
    "enable": true,
    "ignored": {
      "async-trait": ["async_trait"],
      "napi-derive": ["napi"],
      "async-recursion": ["async_recursion"]
    }
  }
}
